// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"content":{
			"bsonType": "string",
			"maxLength": 5000,
			"description": "note content"
		},
		"user_name":{
			"bsonType": "string",
			"description": "Corresponding user"
		},
		"createdtime":{
			"bsonType": "timestamp",
			"defaultValue":{
				"$env": "now"
			}
		}
		
	}
}