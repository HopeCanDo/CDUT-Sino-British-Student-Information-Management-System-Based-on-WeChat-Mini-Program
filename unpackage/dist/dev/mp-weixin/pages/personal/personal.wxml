<view><text class="title">personal information</text><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index"><view class="menu"><button data-event-opts="{{[['tap',[['',['$event']]]]]}}" bindtap="__e"><view class="menu-item"><view value="{{user_name}}" data-event-opts="{{[['input',[['__set_model',['','user_name','$event',[]]]]]]}}" bindinput="__e">User:<text class="con">{{''+user_name}}</text></view></view></button><button data-event-opts="{{[['tap',[['StudentID']]]]}}" bindtap="__e"><view class="menu-item"><view value="{{studentID}}" data-event-opts="{{[['input',[['__set_model',['','studentID','$event',[]]]]]]}}" bindinput="__e">StudentID:<text class="con">{{''+item.studentID}}</text></view></view></button><button data-event-opts="{{[['tap',[['Gender']]]]}}" bindtap="__e"><view class="menu-item"><view>Gender:<text class="con">{{''+item.gender}}</text></view></view></button><button data-event-opts="{{[['tap',[['Class']]]]}}" bindtap="__e"><view class="menu-item"><view>Class:<text class="con">{{''+item.class}}</text></view></view></button><button data-event-opts="{{[['tap',[['ChineseName']]]]}}" bindtap="__e"><view class="menu-item"><view>Chinese name:<text class="con">{{item.chinese_name}}</text></view></view></button><button data-event-opts="{{[['tap',[['EnglishName']]]]}}" bindtap="__e"><view class="menu-item"><view>English name:<text class="con">{{item.english_name}}</text></view></view></button><button data-event-opts="{{[['tap',[['Subject']]]]}}" bindtap="__e"><view class="menu-item"><view>Specialized subject:<text class="con">{{item.subject}}</text></view></view></button><button data-event-opts="{{[['tap',[['Email']]]]}}" bindtap="__e"><view class="menu-item"><view>Email:<text class="con">{{item.student_email}}</text></view></view></button><button data-event-opts="{{[['tap',[['Telephone']]]]}}" bindtap="__e"><view class="menu-item"><view>telephone:<text class="con">{{item.telephone}}</text></view></view></button></view></block></view>