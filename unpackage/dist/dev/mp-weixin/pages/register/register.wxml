<view><swiper indicator-color="white" autoplay="{{true}}" interval="2000" circular="{{true}}"><swiper-item><image src="/static/swiper/icon.png"></image></swiper-item></swiper><text>Username:</text><input type="number" placeholder="Enter your student id" maxlength="12" data-event-opts="{{[['blur',[['doInput',['$0'],['user_name']]]],['input',[['__set_model',['','user_name','$event',[]]]]]]}}" value="{{user_name}}" bindblur="__e" bindinput="__e"/><text>Password:</text><input type="text" value="{{password}}" placeholder="Enter your password" maxlength="16" data-event-opts="{{[['blur',[['doInputpassword',['$0'],['password']]]],['input',[['__set_model',['','password','$event',[]]]]]]}}" bindblur="__e" bindinput="__e"/><button type="default" data-event-opts="{{[['tap',[['finishregister',['$event']]]]]}}" bindtap="__e">Regist</button></view>